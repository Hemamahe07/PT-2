class Sqrt {
    public static void main(String[] args) {
        int x = 16, l = 0, r = x, ans = 0;
        while (l <= r) {
            int m = (l + r) / 2;
            if ((long)m * m <= x) {
                ans = m;
                l = m + 1;
            } else r = m - 1;
        }
        System.out.println(ans);
    }
}
