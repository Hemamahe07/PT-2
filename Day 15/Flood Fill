def floodFill(image, sr, sc, color):
    m,n=len(image),len(image[0])
    start=image[sr][sc]
    if start==color: return image
    def dfs(r,c):
        if r<0 or c<0 or r>=m or c>=n or image[r][c]!=start: return
        image[r][c]=color
        dfs(r+1,c); dfs(r-1,c); dfs(r,c+1); dfs(r,c-1)
    dfs(sr,sc)
    return image
