class LeftRotate {
    static void reverse(int[] a, int l, int r) {
        while (l < r) {
            int t = a[l];
            a[l++] = a[r];
            a[r--] = t;
        }
    }

    public static void main(String[] args) {
        int[] a = {1,2,3,4,5};
        int k = 2;
        k %= a.length;

        reverse(a, 0, k - 1);
        reverse(a, k, a.length - 1);
        reverse(a, 0, a.length - 1);

        for (int i : a) System.out.print(i + " ");
    }
}
