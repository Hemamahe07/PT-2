class MaxDifference {
    public static void main(String[] args) {
        int[] a = {2, 3, 10, 6, 4, 8, 1};
        int min = a[0], maxDiff = a[1] - a[0];

        for (int i = 1; i < a.length; i++) {
            maxDiff = Math.max(maxDiff, a[i] - min);
            min = Math.min(min, a[i]);
        }
        System.out.println(maxDiff);
    }
}
