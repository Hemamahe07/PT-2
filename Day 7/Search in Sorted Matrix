class SearchMatrix {
    public static void main(String[] args) {
        int[][] m = {
            {1,4,7},
            {2,5,8},
            {3,6,9}
        };
        int x = 5, i = 0, j = m[0].length - 1;

        while (i < m.length && j >= 0) {
            if (m[i][j] == x) {
                System.out.println("Found");
                return;
            }
            if (m[i][j] > x) j--;
            else i++;
        }
        System.out.println("Not Found");
    }
}
